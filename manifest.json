{
    "manifest_version": 3,
    "name": "TabTidy",
    "version": "1.0",
    "description": "Automatically close unused tabs after a set period of time.",
    "permissions": [
    "tabs",
    "storage",
    "activeTab",
    "alarms"
  ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    }
  }
  